{
  "miniprogramRoot": "./dist",
  "projectname": "myApp",
  "description": "学习taro",
  "appid": "touristappid",
  "setting": {
    "skylineRenderEnable": true,
    "urlCheck": true,
    "es6": false,
    "enhance": false,
    "compileHotReLoad": false,
